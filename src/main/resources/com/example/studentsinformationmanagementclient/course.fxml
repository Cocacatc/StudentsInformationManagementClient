<AnchorPane prefHeight="550.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.studentsinformationmanagementclient.controller.CourseController">
    <children>
        <ImageView fitHeight="500.0" fitWidth="750.0" layoutY="-155.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@sdu8.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="134.0" fitWidth="109.0" layoutX="641.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@sdu.png" />
            </image>
        </ImageView>
        <TableView fx:id="table" layoutY="234.0" prefHeight="316.0" prefWidth="750.0" />
        <Button fx:id="insertCourseButton" layoutX="30.0" layoutY="77.0" mnemonicParsing="false" onAction="#onInsertCourseAction" style="-fx-background-color: #ffffff; -fx-background-radius: 60;" text="+">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
      <Button fx:id="queryAllButton" layoutX="40.0" layoutY="160.0" mnemonicParsing="false" onAction="#onQueryAllAction" text="查询所有课程" />
      <Button fx:id="queryCourseButton" layoutX="511.0" layoutY="172.0" mnemonicParsing="false" onAction="#onQueryCourseAction" text="查询" />
